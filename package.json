{
  "name": "@luxcium/rpc-worker-pool",
  "version": "1.0.0",
  "description": "Actor Model task queue",
  "main": "dist/index.js",
  "types": "dist/typings/index.d.ts",
  "scripts": {
    "build": "(npm test && bash scripts/build.sh) || exit 15",
    "install": "pwd; yarn build || exit 13; pwd",
    "deploy:local": "cp package.json ./docker/package.json; npm run build && npm run docker:build",
    "docker:build": "docker build --progress plain --label 'image:rpc-worker-pool' --tag rpc-worker-pool:latest ./docker",
    "docker:live:server": "zsh scripts/docker-run.server.sh || exit 55",
    "docker:live:actor": "zsh scripts/docker-run.actor.sh || exit 66",
    "stop:docker:live:server": "docker stop worker-pool-live-server || exit 75",
    "stop:docker:live:actor": "docker stop actor-pool-live || exit 76",
    "stop:docker:live": "npm run stop:docker:live:server; npm run stop:docker:live:actor",
    "docker:server:live:": "MORE_ARGS=\"-it --user 1000 -l 'live:server-pool' --name 'worker-pool-live' rpc-worker-pool:latest\" npm run docker:run:base || exit 55",
    "test:": "jest --config='config/jest.config.json'",
    "test": "echo Tests unimplemented; exit 0"
  },
  "author": "Benjamin Vincent",
  "license": "MIT",
  "dependencies": {
    "chalk": "4.1.2",
    "@luxcium/bigintstring": "workspace:*",
    "@luxcium/tools": "workspace:*",
    "@luxcium/redis-services": "workspace:*",
    "mapping-tools": "workspace:*",
    "@luxcium/phash-compute": "workspace:*"
  },
  "devDependencies": {
    "@babel/core": "~7.22.5",
    "@babel/plugin-transform-typescript": "~7.22.5",
    "@rushstack/eslint-config": "~3.3.2",
    "@rushstack/heft": "~0.56.0",
    "@rushstack/heft-jest-plugin": "~0.7.11",
    "@types/chai": "~4.3.5",
    "@types/eslint-config-prettier": "~6.11.0",
    "@types/eslint-plugin-prettier": "~3.1.0",
    "@types/express": "~4.17.17",
    "@types/fs-extra": "~11.0.1",
    "@types/glob": "~8.1.0",
    "@types/heft-jest": "~1.0.3",
    "@types/mime-types": "~2.1.1",
    "@types/prettier": "~2.7.3",
    "@typescript-eslint/eslint-plugin": "~5.60.1",
    "@typescript-eslint/parser": "~5.60.1",
    "chai": "~4.3.7",
    "concurrently": "~8.0.1",
    "eslint": "~8.44.0",
    "eslint-config-prettier": "~8.8.0",
    "eslint-import-resolver-typescript": "~3.5.5",
    "eslint-plugin-import": "~2.27.5",
    "eslint-plugin-jsdoc": "~46.4.3",
    "eslint-plugin-jsonc": "~2.9.0",
    "eslint-plugin-prettier": "~4.2.1",
    "eslint-plugin-react": "~7.32.2",
    "eslint-plugin-react-hooks": "~4.6.0",
    "eslint-plugin-simple-import-sort": "~10.0.0",
    "eslint-plugin-tsdoc": "~0.2.17",
    "eslint-plugin-unicorn": "~47.0.0",
    "nodemon": "~2.0.22",
    "prettier": "~3.0.0",
    "rimraf": "~5.0.0",
    "ts-loader": "~9.4.4",
    "ts-node": "~10.9.1",
    "tsconfig-paths": "~4.2.0",
    "typescript": "~5.1.6",
