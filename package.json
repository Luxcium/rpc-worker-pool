{
  "name": "@luxcium/rpc-worker-pool",
  "version": "1.0.0",
  "description": "Actor Model task queue",
  "main": "dist/index.js",
  "types": "dist/typings/index.d.ts",
  "scripts": {
    "build": "(npm test && bash scripts/build.sh) || exit 0 # 15",
    "debug": "DEBUG_MODE=true node --inspect-brk -r ts-node/register src/main.ts",
    "prettier": "prettier --write .",
    "lint": " eslint ./src",
    "lint:fix": "eslint ./src -v --fix",
    "install": "pwd; yarn build || exit 13; pwd",
    "deploy:local": "cp package.json ./docker/package.json; npm run build && npm run docker:build",
    "docker:build": "docker build --progress plain --label 'image:rpc-worker-pool' --tag rpc-worker-pool:latest ./docker",
    "docker:live:server": "zsh scripts/docker-run.server.sh || exit 55",
    "docker:live:actor": "zsh scripts/docker-run.actor.sh || exit 66",
    "stop:docker:live:server": "docker stop worker-pool-live-server || exit 75",
    "stop:docker:live:actor": "docker stop actor-pool-live || exit 76",
    "stop:docker:live": "npm run stop:docker:live:server; npm run stop:docker:live:actor",
    "docker:server:live:": "MORE_ARGS=\"-it --user 1000 -l 'live:server-pool' --name 'worker-pool-live' rpc-worker-pool:latest\" npm run docker:run:base || exit 55",
    "test:": "jest --config='config/jest.config.json'",
    "test": "echo Tests unimplemented; exit 0"
  },
  "author": "Benjamin Vincent",
  "license": "MIT",
  "dependencies": {
    "chalk": "4.1.2",
    "@luxcium/bigintstring": "workspace:*",
    "@luxcium/tools": "workspace:*",
    "@luxcium/redis-services": "workspace:*",
    "mapping-tools": "workspace:*",
    "@luxcium/phash-compute": "workspace:*"
  },
  "devDependencies": {
    "@babel/core": "~7.23.9",
    "@babel/plugin-transform-react-jsx": "~7.22.5",
    "@babel/plugin-transform-typescript": "~7.23.6",
    "@jest/globals": "~29.7.0",
    "@rushstack/eslint-config": "~3.3.2",
    "@rushstack/eslint-plugin": "~0.14.0",
    "@rushstack/heft-jest-plugin": "~0.10.6",
    "@rushstack/heft": "~0.63.4",
    "@types/chai": "~4.3.5",
    "@types/eslint-config-prettier": "~6.11.3",
    "@types/eslint-plugin-prettier": "~3.1.3",
    "@types/eslint": "~8.56.2",
    "@types/express": "~4.17.21",
    "@types/fs-extra": "~11.0.1",
    "@types/glob": "~8.1.0",
    "@types/heft-jest": "~1.0.3",
    "@types/jest": "~29.5.12",
    "@types/mime-types": "~2.1.1",
    "@types/node": "~22.7.4",
    "@types/react-dom": "~18.3.0",
    "@types/react": "~18.3.11",
    "@typescript-eslint/eslint-plugin": "~8.8.0",
    "@typescript-eslint/parser": "~8.8.0",
    "chai": "~4.3.7",
    "concurrently": "~8.0.1",
    "eslint-config-next": "~14.2.14",
    "eslint-config-prettier": "~9.1.0",
    "eslint-import-resolver-typescript": "~3.6.1",
    "eslint-plugin-import": "~2.31.0",
    "eslint-plugin-jsdoc": "~48.0.5",
    "eslint-plugin-jsonc": "~2.13.0",
    "eslint-plugin-prettier": "~5.2.1",
    "eslint-plugin-promise": "~6.1.1",
    "eslint-plugin-react-hooks": "~4.6.2",
    "eslint-plugin-react": "~7.37.1",
    "eslint-plugin-simple-import-sort": "~12.1.1",
    "eslint-plugin-tsdoc": "~0.2.17",
    "eslint-plugin-unicorn": "~51.0.0",
    "eslint": "~9.11.1",
    "jest": "~29.7.0",
    "nodemon": "~3.0.3",
    "prettier": "~3.3.3",
    "rimraf": "~6.0.1",
    "ts-jest": "~29.1.2",
    "ts-node": "~10.9.2",
    "tsconfig-paths": "~4.2.0",
    "tslib": "~2.6.2",
    "typescript": "~5.6.2",
    "@stylistic/eslint-plugin": "~1.6.1",
    "@eslint-stylistic/metadata": "~1.6.1",
    "prettier-plugin-organize-attributes": "~1.0.0",
    "prettier-plugin-packagejson": "~2.5.2",
    "prettier-plugin-sh": "~0.14.0"
  }
}
